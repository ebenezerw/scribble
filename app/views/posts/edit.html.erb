<div class="well">
<h2>Edit Post</h2>

<%= form_for :post, url: post_path(@post), method: :patch do |f| %>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>
  <%= f.label :author %><br>
  <%= f.text_field :author %>
<p>
  <%= f.label :content %><br>
  <%= f.text_area :content %>
</p>
<p>
  <%= f.submit "Save" %>
</p>
</div>
<% end %>
