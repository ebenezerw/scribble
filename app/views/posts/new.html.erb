<div class="well">
<h2>Add Post</h2>

<%= form_for :post, url: posts_path do |f| %>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>
  <%= f.label :author %><br>
  <%= f.text_field :author %>
<p>
  <%= f.label :content %><br>
  <%= f.text_area :content %>
</p>
<p>
  <%= f.submit "Create" %>
</p>
</div>
<% end %>
